{
  "name": "@oberd/oui",
  "version": "0.0.1",
  "description": "Oberd Components FTW",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/oui/oui.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "update-readme": "node scripts/appendComponentList.js",
    "build": "stencil build --docs && npm run update-readme",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "npm run build && concurrently 'npm run start' 'npm run storybook:start'",
    "storybook:start": "start-storybook -s ./www,./static -p 6006"
  },
  "devDependencies": {
    "@babel/core": "7.11.0",
    "@intuit/renovate-config": "1.2.4",
    "@stencil/core": "1.17.2",
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-links": "5.3.19",
    "@storybook/addons": "5.3.19",
    "@storybook/react": "5.3.19",
    "@types/jest": "26.0.7",
    "@types/puppeteer": "3.0.1",
    "babel-loader": "8.1.0",
    "concurrently": "5.2.0",
    "github-markdown-css": "4.0.0",
    "jest": "26.2.2",
    "jest-cli": "26.2.2",
    "marked": "1.1.1",
    "puppeteer": "5.2.1",
    "stencil-inline-svg": "1.0.1",
    "tslint": "6.1.3",
    "webpack-watch-files-plugin": "1.0.3"
  },
  "license": "MIT"
}
