        "test": "stencil test --spec --e2e",
        "test.watch": "stencil test --spec --e2e --watchAll",
        "generate": "stencil generate",
        "svg": "node scripts/svg.mjs"
    },
    "devDependencies": {
        "@stencil/core": "^2.3.0",
        "@types/jest": "26.0.15",
        "@types/puppeteer": "5.4.0",
        "jest": "26.6.3",
        "jest-cli": "26.6.3",
        "lodash.camelcase": "^4.3.0",
        "prettier": "^2.2.0",
        "puppeteer": "5.5.0",
        "stencil-inline-svg": "^1.0.1"
    },
    "license": "MIT"
}
