@keyframes spin {
    0% {
      transform: rotate(0deg);
    }
    60% {
      transform: rotate(360deg);
    }
    100% {
      transform: rotate(360deg);
    }
}
.a-spin {
  animation: spin 1s;
  animation-timing-function: ease-in-out;
  animation-iteration-count: infinite;
  display: inline-block;
  transform-origin: 45% 42%;
}
